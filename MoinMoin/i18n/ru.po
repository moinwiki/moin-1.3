#, python-format
msgid "Edit \"%(pagename)s\""
msgstr "Правка \"%(pagename)s\""

#, python-format
msgid "Preview of \"%(pagename)s\""
msgstr "Макет \"%(pagename)s\""

#, python-format
msgid "Your edit lock on %(lock_page)s has expired!"
msgstr "Привилегия на изменение %(lock_page)s истекла!"

