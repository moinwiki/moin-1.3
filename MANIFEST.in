# additional files not known by setup.py
include CHANGES COPYING INSTALL.html UPDATE.html README TODO
include MANIFEST.in moinlogo.bmp setup.cfg

# "wiki" directory
recursive-include   wiki/cgi-bin *.cgi *.py
include             wiki/data/intermap.txt
recursive-include   wiki/data/text *
include             wiki/htdocs/index.html
recursive-include   wiki/htdocs/css *.css
recursive-include   wiki/htdocs/img *.gif *.png
recursive-include   wiki/htdocs/applets *.jar

# include MoinMoin/wiki-moinmoin/default.css
# include MoinMoin/wiki-moinmoin/data/editlog
# recursive-include MoinMoin/wiki-moinmoin/img *
# recursive-include MoinMoin/wiki-moinmoin/data/text *

# contributions
recursive-include contributions *

# omit CVS directories
global-exclude */CVS/*

